{
    "name": ">Sharepoint< Spelling Edge Extension",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "An extension to highlight when SharePoint is mis-capitialized, argh",
    "permissions": [
        "tabs",
        "activeTab",
        "webNavigation",
        "scripting"
    ],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"],
          "run_at": "document_idle"
        }
      ]   
}